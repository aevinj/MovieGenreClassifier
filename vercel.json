{
    "builds": [
      {
        "src": "MovieGenreClassifier/MovieGenreClassifier/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLamdaSize": "15mb", "runtime": "python3.11"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "MovieGenreClassifier/MovieGenreClassifier/wsgi.py"
      }
    ]
}